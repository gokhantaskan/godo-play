{
  "name": "godo-play",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "build:dev": "env-cmd --expand-envs --verbose -f .env nuxt build",
    "build:prod": "env-cmd --expand-envs --verbose -f .env.production nuxt build",
    "dev": "pnpm dev:db && pnpm dev:client",
    "dev:prod": "env-cmd --expand-envs -f .env.production sh -c 'pnpm db:push && pnpm dev:client'",
    "dev:client": "nuxt dev",
    "dev:db": "./scripts/dev-setup.sh",
    "dev:clean": "nuxt cleanup && pnpm dev",
    "generate": "nuxt generate",
    "preview": "NITRO_SSL_CERT=$(cat localhost+2.pem) NITRO_SSL_KEY=$(cat localhost+2-key.pem) nuxt preview",
    "typecheck": "nuxt typecheck",
    "clean": "pnpm clean:nuxt && rimraf --glob **/dist **/inspect",
    "clean:nuxt": "nuxt cleanup",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "lint": "eslint . --fix",
    "format": "prettier --write '**/*.{html,json,md,css,scss}' -l -u",
    "postinstall": "nuxt prepare",
    "slicemachine": "start-slicemachine",
    "prepare": "husky"
  },
  "packageManager": "pnpm@9.15.4",
  "engines": {
    "node": "22"
  },
  "dependencies": {
    "@floating-ui/vue": "^1.1.6",
    "@headlessui/vue": "^1.7.23",
    "@nuxt/eslint": "1.1.0",
    "@nuxt/fonts": "0.10.3",
    "@nuxt/icon": "1.10.3",
    "@nuxt/image": "1.9.0",
    "@nuxt/scripts": "0.10.5",
    "@nuxtjs/robots": "5.2.4",
    "@nuxtjs/sitemap": "7.2.6",
    "@nuxtjs/supabase": "1.4.6",
    "@prismicio/client": "^7.16.1",
    "@prismicio/vue": "^5.1.0",
    "@supabase/ssr": "^0.5.2",
    "@supabase/supabase-js": "^2.49.1",
    "@tiptap/pm": "^2.11.5",
    "@tiptap/starter-kit": "^2.11.5",
    "@tiptap/vue-3": "^2.11.5",
    "@vee-validate/zod": "^4.15.0",
    "@vercel/analytics": "^1.5.0",
    "@vueuse/core": "^12.7.0",
    "dompurify": "^3.2.4",
    "drizzle-orm": "^0.40.0",
    "drizzle-seed": "^0.3.1",
    "eslint": "^9.21.0",
    "ioredis": "^5.5.0",
    "jsdom": "^26.0.0",
    "lodash": "^4.17.21",
    "nuxt": "^3.15.4",
    "nuxt-gtag": "3.0.2",
    "nuxt-schema-org": "4.1.3",
    "nuxt-security": "2.5.1",
    "pg": "^8.13.3",
    "reka-ui": "^2.0.2",
    "slugify": "^1.6.6",
    "tailwindcss": "4.0.0-beta.8",
    "unstorage": "^1.15.0",
    "vee-validate": "^4.15.0",
    "vue": "latest",
    "vue-router": "latest",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@iconify-json/fa-solid": "^1.2.1",
    "@iconify-json/fa6-brands": "^1.2.5",
    "@iconify-json/lucide": "^1.2.28",
    "@nuxtjs/prismic": "^4.0.1",
    "@slicemachine/adapter-nuxt": "^0.3.68",
    "@tailwindcss/postcss": "^4.0.9",
    "@types/jsdom": "^21.1.7",
    "@types/lodash": "^4.17.16",
    "@types/pg": "^8.11.11",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "dotenv-expand": "^12.0.1",
    "drizzle-kit": "^0.30.5",
    "drizzle-zod": "^0.7.0",
    "env-cmd": "^10.1.0",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "postcss-preset-env": "^10.1.5",
    "prettier": "^3.5.2",
    "rimraf": "^6.0.1",
    "sass-embedded": "^1.85.1",
    "slice-machine-ui": "^2.13.1",
    "typescript": "5.7.3",
    "vite-svg-loader": "^5.1.0",
    "vue-tsc": "~2.1.10"
  }
}

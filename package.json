{
  "name": "godo-play",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "build:dev": "env-cmd --expand-envs --verbose -f .env nuxt build",
    "build:prod": "env-cmd --expand-envs --verbose -f .env.production nuxt build",
    "dev": "pnpm dev:db && pnpm dev:client",
    "dev:client": "nuxt dev",
    "dev:db": "./scripts/dev-setup.sh",
    "dev:clean": "nuxt cleanup && pnpm dev",
    "generate": "nuxt generate",
    "preview": "NITRO_SSL_CERT=$(cat localhost+2.pem) NITRO_SSL_KEY=$(cat localhost+2-key.pem) nuxt preview",
    "typecheck": "nuxt typecheck",
    "clean": "pnpm clean:nuxt && rimraf --glob **/dist **/inspect",
    "clean:nuxt": "nuxt cleanup",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "lint": "eslint . --fix",
    "format": "prettier --write '**/*.{html,json,md,css,scss}' -l -u",
    "postinstall": "nuxt prepare",
    "slicemachine": "start-slicemachine",
    "prepare": "husky"
  },
